{
  "name": "vatscan",
  "version": "0.7.14",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "devDependencies": {
    "babel-eslint": "latest",
    "babel-preset-expo": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "jest-expo": "^36.0.0",
    "prettier": "latest",
    "react-test-renderer": "16.9.0"
  },
  "scripts": {
    "start": "expo start",
    "eject": "expo eject",
    "build-android": "expo build:android",
    "build-ios": "expo build:ios",
    "test": "node_modules/.bin/jest",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "format": "node_modules/.bin/prettier --config ./.prettierrc --write ./App.js ./src/**/*.js",
    "eslint": "node_modules/.bin/eslint App.js src/**/*.js"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "plugin:react/recommended",
      "airbnb"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "react/jsx-filename-extension": "off",
      "react/state-in-constructor": "off"
    }
  },
  "eslintIgnore": [
    "src/data/*"
  ],
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/netinfo": "4.6.0",
    "expo": "^36.0.0",
    "expo-font": "~8.0.0",
    "moment": "latest",
    "react": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.1.tar.gz",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-maps": "0.26.1",
    "react-native-paper": "latest",
    "react-native-reanimated": "~1.4.0",
    "react-navigation": "latest",
    "react-navigation-slide-from-right-transition": "latest",
    "react-navigation-stack": "latest",
    "react-navigation-tabs": "latest"
  }
}
